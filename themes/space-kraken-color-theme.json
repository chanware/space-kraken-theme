{
	"name": "Space Kraken",
	"colors": {
		// Core Editor - Deep space backdrop with stellar text
		"editor.background": "#112128",
		"editor.foreground": "#cdd3de",
		
		// Activity Bar - Kraken's domain
		"activityBar.background": "#152025",
		"activityBar.foreground": "#607d8b",
		"activityBarBadge.background": "#82aaff",
		
		// Side Bar - Navigation through the void
		"sideBar.background": "#152025",
		"sideBar.foreground": "#607d8b",
		"sideBarTitle.foreground": "#cdd3de",
		"sideBarSectionHeader.background": "#1e272c",
		
		// Status Bar - Mission control
		"statusBar.background": "#1e272c",
		"statusBar.foreground": "#607d8b",
		"statusBar.noFolderBackground": "#1e272c",
		"statusBar.debuggingBackground": "#2a4a5a",
		"statusBarItem.hoverBackground": "#2e3c43",
		"statusBarItem.activeBackground": "#31424d",
		"statusBarItem.prominentBackground": "#3d5a6b",
		"statusBarItem.prominentHoverBackground": "#4a6b7d",
		
		// Title Bar - Command bridge
		"titleBar.activeBackground": "#1e272c",
		"titleBar.activeForeground": "#cdd3de",
		"titleBar.inactiveBackground": "#152025",
		"titleBar.inactiveForeground": "#425b67",
		
		// Editor Selection & Highlights - Kraken's tentacles
		"editor.selectionBackground": "#31424d",
		"editor.lineHighlightBackground": "#0d1a21",
		"editor.findMatchBackground": "#29353f",
		"editor.findMatchHighlightBackground": "#252f3b",
		"editor.wordHighlightBackground": "#252f3b",
		"editor.wordHighlightStrongBackground": "#31424d",
		
		// Cursor - Bioluminescent marker
		"editorCursor.foreground": "#b6c4ff",
		
		// Line Numbers - Coordinate system
		"editorLineNumber.foreground": "#364555",
		"editorLineNumber.activeForeground": "#607d86",
		
		// Gutter - Version control in the abyss
		"editorGutter.background": "#112128",
		"editorGutter.addedBackground": "#174e26",
		"editorGutter.deletedBackground": "#80202b",
		"editorGutter.modifiedBackground": "#5a3625",
		
		// Tabs - File constellation
		"tab.activeBackground": "#293c46",
		"tab.activeForeground": "#cdd3de",
		"tab.inactiveBackground": "#152025",
		"tab.inactiveForeground": "#607d8b",
		"tab.border": "#1a2d34",
		
		// Panel - Control center
		"panel.background": "#152025",
		"panel.border": "#22343d",
		"panelTitle.activeForeground": "#cdd3de",
		"panelTitle.inactiveForeground": "#607d8b",
		
		// Terminal - Communication portal
		"terminal.background": "#152025",
		"terminal.foreground": "#c3cee3",
		
		// Scrollbar - Navigation aids
		"scrollbar.shadow": "#22343b00",
		"scrollbarSlider.background": "#5a7380d4",
		"scrollbarSlider.hoverBackground": "#6c8795d4",
		"scrollbarSlider.activeBackground": "#6c8795d4",
		
		// Borders & Separators - Structural elements
		"border": "#22343d",
		"separator": "#3b4950",
		
		// Notifications - Incoming transmissions
		"notification.background": "#3d1a11",
		
		// Input - Data entry fields
		"input.background": "#1f2e38",
		"input.border": "#22343d",
		"input.foreground": "#cdd3de",
		
		// Button - Action triggers
		"button.background": "#82aaff",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#85c3f4",
		
		// Dropdown - Selection menus
		"dropdown.background": "#1f2e38",
		"dropdown.border": "#22343d",
		"dropdown.foreground": "#cdd3de"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#4d6470"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#cdd3de"
			}
		},
		{
			"name": "Local Variables",
			"scope": [
				"variable.other.local"
			],
			"settings": {
				"foreground": "#cdd3de"
			}
		},
		{
			"name": "Parameters",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#fb9551"
			}
		},
		{
			"name": "Instance Fields",
			"scope": [
				"variable.other.property",
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#97a0b4"
			}
		},
		{
			"name": "Global Variables",
			"scope": [
				"variable.other.global"
			],
			"settings": {
				"foreground": "#e4c16c"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff5370"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#dca9dc"
			}
		},
		{
			"name": "Control Keywords",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#dca9dc",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Operators, Punctuation",
			"scope": [
				"punctuation",
				"keyword.operator",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#66c7c7"
			}
		},
		{
			"name": "Commas",
			"scope": [
				"punctuation.separator.comma"
			],
			"settings": {
				"foreground": "#89ddff"
			}
		},
		{
			"name": "Dots",
			"scope": [
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#89ddff"
			}
		},
		{
			"name": "Semicolons",
			"scope": [
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#60b3b3"
			}
		},
		{
			"name": "Braces, Brackets, Parentheses",
			"scope": [
				"punctuation.section.braces",
				"punctuation.section.brackets",
				"punctuation.section.parens"
			],
			"settings": {
				"foreground": "#66c7c7"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f36e77"
			}
		},
		{
			"name": "Function Names",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function"
			],
			"settings": {
				"foreground": "#85c3f4"
			}
		},
		{
			"name": "Method Calls",
			"scope": [
				"entity.name.method"
			],
			"settings": {
				"foreground": "#85c3f4"
			}
		},
		{
			"name": "Static Methods",
			"scope": [
				"entity.name.function.static"
			],
			"settings": {
				"foreground": "#85c3f4",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Numbers and Constants",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#fb9551"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"markup.heading",
				"markup.inserted.git_gutter"
			],
			"settings": {
				"foreground": "#99c794"
			}
		},
		{
			"name": "String Escapes",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#85c3f4"
			}
		},
		{
			"name": "Class Names",
			"scope": [
				"entity.name.class",
				"entity.name.type",
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#e4c16c"
			}
		},
		{
			"name": "Abstract Classes",
			"scope": [
				"entity.name.type.class.abstract"
			],
			"settings": {
				"foreground": "#80cbc4"
			}
		},
		{
			"name": "Interface Names",
			"scope": [
				"entity.name.type.interface"
			],
			"settings": {
				"foreground": "#99c794",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Type Parameters",
			"scope": [
				"entity.name.type.parameter"
			],
			"settings": {
				"foreground": "#80cbc4",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Attributes/Annotations",
			"scope": [
				"entity.other.attribute-name",
				"entity.name.function.decorator"
			],
			"settings": {
				"foreground": "#dca9dc",
				"fontStyle": "italic"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#dca9dc"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#dca9dc",
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Properties",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#e4c16c"
			}
		},
		{
			"name": "CSS Values",
			"scope": [
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#fb9551"
			}
		},
		{
			"name": "Markup - Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#99c794"
			}
		},
		{
			"name": "Markup - Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#f36e77"
			}
		},
		{
			"name": "Markup - Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#c792ea"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#f36e77"
			}
		},
		{
			"name": "JSON Keys",
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#dca9dc"
			}
		},
		{
			"name": "Markdown Headings",
			"scope": [
				"markup.heading"
			],
			"settings": {
				"foreground": "#80cbc4",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Code",
			"scope": [
				"markup.inline.raw"
			],
			"settings": {
				"foreground": "#c792ea"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"foreground": "#f36e77",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#f36e77",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown Links",
			"scope": [
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#82aaff"
			}
		}
	]
}